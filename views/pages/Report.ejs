<% include ../partials/header %>


<div class="row">
    <div class="col-2">
        <a href="/products" class="h1 text-dark">&lt; Back</a>
    </div>
    <div class="col-10">
        <h1><%= report[0].title %> Report</h1> 
    </div>
</div>


<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Purchase_id</th>
            <th>Quantity</th>
            <th>Total Price</th>
        </tr>
    </thead>
    <tbody id="myTable">
    <% report.forEach(function(report) { %>
        <tr>
            <td scope="row"><%= report.purchase_id %></td>
            <td><%= report.quantity %></td>
            <td><%= report.price*report.quantity %></td>
            
        </tr>
        <% }); %>
    </tbody>
</table>



<% include ../partials/footer %>