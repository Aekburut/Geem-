<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>Users ID</th>
            <th>Email</th>
            <th>Password</th>
            <th></th>
            <th> <a class="btn btn-success" href="/insert" role="button">Add user</a></th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user) { %>
        <tr>
            <td scope="row"><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
            <td>
                <a class="btn btn-info" href="/users/<%= user.id%>" role="button">Edit</a>
                </td>
                <td>
                <a class="btn btn-danger" href="" role="button">Delete</a>
                </td>
                <td>
                    <a class="btn btn-warning" href="/product_report/<%= product.id%>" role="button">Report</a>
                </td>
        </tr>
        <% }); %>
    </tbody>
</table>



<% include ../partials/footer %>