<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>Users ID</th>
            <th>Email</th>
            <th>Password</th>
            <th>Delete</th>
            <th> <a class="btn btn-success" href="/insert" role="button">Add Products</a></th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user) { %>
        <tr>
            <td scope="row"><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
        </tr>
        <% }); %>
    </tbody>
</table>



<% include ../partials/footer %>